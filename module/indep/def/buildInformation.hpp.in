#pragma once

//  Generated by CMake's configure_file():
//      refers .in file to fix.
//          why do we notion like this format? 1 ~~ % 100?:
//              Imagine that today is 8th July. Then CMake kindly generate BY_BUILD_DATE_DAY's value to 08, and boom.
//              beautiful compile error make a grin to you :)
//              To avoid compiler assume that value to "octal" expression, we should do some "tricky" thing.
#define BY_BUILD_DATE_YEAR         @BUILD_DATE_YEAR@
#define BY_BUILD_DATE_MONTH        (1@BUILD_DATE_MONTH@ % 100)  //  == @BUILD_DATE_MONTH@
#define BY_BUILD_DATE_DAY          (1@BUILD_DATE_DAY@ % 100)    //  == @BUILD_DATE_DAY@
#define BY_BUILD_DATE              "@BUILD_DATE@"
#define BY_BUILD_TIME_HOUR         (1@BUILD_TIME_HOUR@ % 100)    //  == @BUILD_TIME_HOUR@
#define BY_BUILD_TIME_MINUTE       (1@BUILD_TIME_MINUTE@ % 100)  //  == @BUILD_TIME_MINUTE@
#define BY_BUILD_TIME_SECOND       (1@BUILD_TIME_SECOND@ % 100)  //  == @BUILD_TIME_SECOND@
#define BY_BUILD_TIME              "@BUILD_TIME@"
#define BY_BUILD_VERSION_MAJOR     @VERSION_MAJOR@
#define BY_BUILD_VERSION_MINOR     @VERSION_MINOR@
#define BY_BUILD_VERSION_FIX       @VERSION_FIX@
#define BY_BUILD_VERSION_NAME      "@VERSION_NAME@"
#define BY_BUILD_VERSION_VALUE     "@VERSION_VALUE@"
#define BY_BUILD_COUNT             @BUILD_COUNT@
#define BY_BUILD_VERSION           "@VERSION@"
#define BY_BUILD_TYPENAME          "@CMAKE_BUILD_TYPE@"
#define BY_IS_DBG                  @CMAKE_IS_DBG@
#define BY_BUILD_PLATFORM_NAME     "@CMAKE_SYSTEM_NAME@"
#define BY_BUILD_PLATFORM_VERSION  "@CMAKE_SYSTEM_VERSION@"
#define BY_TYPE_LINUX              1
#define BY_TYPE_WINDOWS            2
#define BY_TYPE_MACOS              3
#ifdef __linux__
#    define BY_BUILD_PLATFORM       BY_TYPE_LINUX
#    define BY_BUILD_PLATFORM_IS_LINUX
#endif
#ifdef WIN32
#    define BY_BUILD_PLATFORM        BY_TYPE_WINDOWS
#    define BY_BUILD_PLATFORM_IS_WINDOWS
#endif
#if defined(__APPLE__) && defined(__MACH__)
#   define BY_BUILD_PLATFORM       BY_TYPE_MACOS
#   define BY_BUILD_PLATFORM_IS_MAC
#endif
