pack wrd.lang.example.borg

def borg
    _sharedState := node[]? // nullable to array
    state := ""

    me(): state = "Init"
    as() str: borg.state

def yourBorg from borg()

main() void
    rm1 := borg()
    rm2 := borg()

    rm1.state = "Idle"
    rm2.state = "Running"

    with sys.cons
        out("rm1: $rm1, rm2: $rm2")

        rm2.state = "Zombie"
        out("4m1: $rm1, rm2: $rm2")
        if rm1 === rm2
            out("rm1 is rm2")

    rm3 := yourBorg()
